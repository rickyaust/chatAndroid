<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorOscuro</item>
        <item name="colorPrimaryDark">@color/colorMenosOscuro</item>
        <item name="colorAccent">@color/colorblanco</item>
    </style>
    <style name="AppTheme.NoActionBar">
      <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


    <style name="AppTheme.PopupOverlay" parent="Theme.AppCompat.Light"></style>
    <style name="AppTheme.appBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"></style>
</resources>
